{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css", 
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "api/call-external.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/admin.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/call-external",
      "dest": "/api/call-external.py"
    },
    {
      "src": "/api/admin",
      "dest": "/api/admin.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
